generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id           Int      @id @default(autoincrement())
  telegramId   String   @unique
  firstName    String?
  lastName     String?
  username     String?
  regionIndex  Int
  createdAt    DateTime @default(now())
}
